<Window x:Name="wd_edit" x:Class="filreader.editor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:filreader"
        mc:Ignorable="d"
        Title="editor" Height="450" Width="800" Closed="wd_edit_Closed">
    <Grid>
        <TextBox x:Name="tb_file" HorizontalAlignment="Left" Height="401" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="701"/>
        <Button x:Name="bt_safe" Content="Button" HorizontalAlignment="Left" Margin="716,390,0,0" VerticalAlignment="Top" Width="54" Click="bt_safe_Click"/>

    </Grid>
</Window>
